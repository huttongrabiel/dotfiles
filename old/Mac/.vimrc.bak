syntax on
""set termguicolors
set background=dark
set belloff=all
highlight Normal     ctermbg=NONE guibg=NONE
highlight LineNr     ctermbg=NONE guibg=NONE
highlight SignColumn ctermbg=NONE guibg=NONE
inoremap kj <esc>
set backspace=indent,eol,start
set tabstop=2 softtabstop=2
set shiftwidth=2
set expandtab
set smartindent
set nu
set nowrap
set smartcase
set ruler
set incsearch
set belloff=all
